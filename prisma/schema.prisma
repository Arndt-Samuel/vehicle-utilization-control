// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Driver {
  id String @id @default(uuid())
  name String 

  @@map("driver")
}

model Automobile {
  id String @id @default(uuid())
  plate String
  color String
  brand String

  @@map("automobile")
}